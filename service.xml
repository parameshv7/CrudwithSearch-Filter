<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.bus.service">
	<namespace>FOO</namespace>
	<entity local-service="true" name="Bus" remote-service="true" uuid="true">
		<column name="busId" primary="true" type="long" id-type="increment" />
		<column name="busName" type="String" />
		<column name="busCompany" type="String" />
		<column name="busPrice" type="long" />
	</entity>
</service-builder>